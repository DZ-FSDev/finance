package com.dzappz.finance.domain.spotMarkets;

import java.math.BigInteger;
import lombok.Getter;
import lombok.Setter;

/**
 * Abstract data type for basic Candlesticks on Spot Markets
 * @author DzAppz
 * @version 0.1
 */

public abstract class SpotCandlestick {
	public @Getter @Setter BigInteger open;
	public @Getter @Setter BigInteger close;
	public @Getter @Setter BigInteger high;
	public @Getter @Setter BigInteger low;
	public @Getter @Setter BigInteger quoteVolume;
	public @Getter @Setter BigInteger assetVolume;
}
